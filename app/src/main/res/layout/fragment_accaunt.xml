<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.Fragment_accaunt"
    android:orientation="vertical">
    <LinearLayout
        android:padding="8dp"
        android:id="@+id/linearLayout_one"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <TextView
           android:layout_gravity="center"
          android:layout_marginStart="10dp"
            android:text="abduxalil.voxidjonov"
            android:textSize="20sp"
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_marginTop="2dp"
            android:layout_gravity="center"
            android:src="@drawable/accaunt_switch"
            android:layout_width="30dp"
            android:layout_height="30dp"
            />
        <ImageView
            android:layout_marginEnd="10dp"
            android:layout_marginStart="70dp"
            android:layout_gravity="center"
            android:src="@drawable/outline_add_box_black_24dp"
            android:id="@+id/tv_nastroyka"
            android:layout_width="30dp"
            android:layout_height="30dp"/>
        <ImageView
            android:layout_marginEnd="10dp"
            android:layout_marginStart="5dp"
            android:layout_gravity="center"
            android:src="@drawable/menu"
            android:layout_width="30dp"
            android:layout_height="30dp"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <androidx.cardview.widget.CardView
            app:cardBackgroundColor="#A70F91"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="20dp"
            android:layout_margin="5dp"
            android:elevation="8dp"
            app:cardCornerRadius="100dp"
            app:contentPadding="1dp">
            <androidx.cardview.widget.CardView
                android:layout_gravity="center"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:elevation="8dp"
                app:cardCornerRadius="100dp">
                <ImageView
                    android:id="@+id/story_images"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:elevation="1dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/accaunt" />
            </androidx.cardview.widget.CardView>
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:layout_marginStart="30dp"
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:text="2"
                android:textSize="30sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="Posts"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginStart="30dp"
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:text="111"
                android:textSize="30sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="Followers"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
    </LinearLayout>
        <LinearLayout
            android:layout_marginStart="30dp"
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:text="96"
                android:textSize="30sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="Following"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

</LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_marginStart="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="Abduxalil Voxidjonov"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="Gamer"
            android:textColor="#9C9C9C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="Telegram:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="t.me/about_me_cv"
            android:textColor="#1B303C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            app:cardBackgroundColor="#EFEFEF"
            android:layout_marginEnd="10dp"
            android:elevation="12dp"
            app:cardCornerRadius="15dp"
            android:layout_centerHorizontal="true"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp">
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="Professional dashboard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:textColor="#9C9C9C"
                    android:text="88 accaunts reached in the last 30 days"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:layout_marginEnd="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <androidx.cardview.widget.CardView
                app:cardBackgroundColor="#EFEFEF"
                app:cardCornerRadius="10dp"
                android:layout_centerHorizontal="true"
                android:layout_width="98dp"
                android:layout_height="35dp">
                <TextView
                    android:gravity="center"
                    android:text="Edit Profile"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardBackgroundColor="#EFEFEF"
                android:elevation="12dp"
                android:layout_marginStart="20dp"
                app:cardCornerRadius="10dp"
                android:layout_centerHorizontal="true"
                android:layout_width="98dp"
                android:layout_height="35dp">
                <TextView
                    android:gravity="center"
                    android:text="Share profile"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardBackgroundColor="#EFEFEF"
                android:elevation="12dp"
                app:cardCornerRadius="10dp"
                android:layout_centerHorizontal="true"
                android:layout_width="98dp"
                android:layout_marginStart="25dp"
                android:layout_height="35dp"
                >
                <TextView
                    android:gravity="center"
                    android:text="Contact"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_marginStart="10dp"
        android:layout_marginEnd="20dp"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">
        >

        <LinearLayout
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="10dp"
                app:cardCornerRadius="100dp">
            <androidx.cardview.widget.CardView
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_gravity="center"
                android:layout_centerHorizontal="true"
                app:cardCornerRadius="100dp">

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:src="@drawable/accaunt" />

            </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginStart="10dp"
                android:text="Birga bo'lamiz"
                android:textSize="10sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:cardBackgroundColor="#959191"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="10dp"
                app:cardCornerRadius="100dp">
            <androidx.cardview.widget.CardView
                android:layout_width="68dp"
                android:layout_height="68dp"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                app:cardCornerRadius="100dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="+"
                    android:gravity="center"
                    android:textSize="40sp" />
            </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:text="new"
                android:textSize="10sp" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="30dp">
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/imageView11"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="60dp"
            android:src="@drawable/grid"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="0dp" />

        <ImageView
           android:id="@+id/imageView2"
            android:layout_width="20dp"
            android:layout_gravity="center"
            android:layout_marginStart="80dp"
            android:layout_height="20dp"
            android:src="@drawable/reels"
            tools:ignore="MissingConstraints" />
        <ImageView
            android:id="@+id/imageView3"
            android:layout_gravity="center"
            android:layout_marginStart="80dp"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/contact"
            tools:ignore="MissingConstraints" />
    </LinearLayout>
    <View
        android:layout_marginTop="10dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_width="100dp"
        android:layout_height="2dp"
        android:background="#000000"/>

    <LinearLayout
        android:layout_marginTop="2dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <ImageView
            android:background="#FFA837"
            android:src="@drawable/accaunt"
            android:layout_width="100dp"
            android:layout_height="100dp"/>
        <ImageView
            android:background="#FFA837"
            android:layout_marginStart="4dp"
            android:src="@drawable/accaunt"
            android:layout_width="100dp"
            android:layout_height="100dp">
        </ImageView>
        <ImageView
            android:background="#FFA837"
            android:layout_marginStart="4dp"
            android:src="@drawable/accaunt"
            android:layout_width="100dp"
            android:layout_height="100dp"/>
    </LinearLayout>

</LinearLayout>
